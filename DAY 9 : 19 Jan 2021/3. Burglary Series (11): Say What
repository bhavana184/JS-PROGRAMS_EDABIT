//https://edabit.com/challenge/7hrjegxToTSF4zAoM
function sayWhat(obj) {
	var res="";var end="";
	for(var i in obj){
		if(i==2)end=end+obj[i];
		res=res+obj[i]+" ";
	}
	res=res+end;
	return res
}
/*
Test Passed: Value == 'Mommy please help please'
Test Passed: Value == '6icfk innocent is innocent'
*/
