//https://edabit.com/challenge/8s2jy9hR2TAeQinKD


function calculateDifference(obj, limit) {

  var sum=0;

  for(var i in obj)

      sum=sum+obj[i];

  return Math.abs(sum-limit)

}
/*
    Test Passed: Value == 1
    Test Passed: Value == 21
    Test Passed: Value == 35
*/

