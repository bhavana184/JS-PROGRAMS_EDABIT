//https://edabit.com/challenge/pPNAs5PvB3WvnDwDM
function toArray(obj) {
	var arr=[];
	for(var i in obj){
		var res=[];
		res.push(i,obj[i]);
		arr.push(res);
	}
	return arr;
}
/*
Test Passed: Value == "[['a', 1], ['b', 2]]"
Test Passed: Value == "[['foo', 33], ['bar', 45], ['baz', 67]]"
Test Passed: Value == "[['shrimp', 15], ['tots', 12]]"
Test Passed: Value == '[]'
*/
/*
Test.assertSimilar(toArray({ a: 1, b: 2 }), [["a", 1], ["b", 2]])
Test.assertSimilar(toArray({ foo: 33, bar: 45, baz: 67 }), [["foo", 33], ["bar", 45], ["baz", 67]])
Test.assertSimilar(toArray({ shrimp: 15, tots: 12 }), [["shrimp", 15], ["tots", 12]])
Test.assertSimilar(toArray({}), [])
*/
