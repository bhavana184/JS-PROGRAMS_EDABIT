//https://edabit.com/challenge/pPNAs5PvB3WvnDwDM - question link

function toArray(obj) {
	var res=[];
	for(var i in obj){
		res.push([i,obj[i]]);
	}
	return res;
}
/*
Test Passed: Value == "[['a', 1], ['b', 2]]"
Test Passed: Value == "[['foo', 33], ['bar', 45], ['baz', 67]]"
Test Passed: Value == "[['shrimp', 15], ['tots', 12]]"
Test Passed: Value == '[]'
*/
