//https://edabit.com/challenge/pLNavsePxJ87t9Nak
function calculateLosses(obj) {
	var sum=0;
	for(var i in obj){
		sum=sum+obj[i];
	}
	if(sum==0)return 'Lucky you!';
	return sum
}
/*
Test Passed: Value == 100
Test Passed: Value == 50001
Test Passed: Value == 3500
Test Passed: Value == 'Lucky you!'
*/
