//https://edabit.com/challenge/4aaBNPnFMc3bzR7JR
function objectToArray(obj) {
	var res=[]
	for(var i in obj){
		var a=[];
		a.push(i,obj[i]);
		res.push(a);
	}
	return res;
}
/*
Test Passed: Value == "[['D', 1], ['B', 2], ['C', 3]]"
Test Passed: Value == "[['likes', 2], ['dislikes', 3], ['followers', 10]]"
*/
