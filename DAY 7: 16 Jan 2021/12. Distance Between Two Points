//https://edabit.com/challenge/caeSeQ3K53GMQKenX
function getDistance(a, b) {
	return parseFloat(Math.sqrt((b.x-a.x)**2 +(b.y-a.y)**2).toFixed(3));
}
/*
Test Passed: Value == 6.325
Test Passed: Value == 1.414
Test Passed: Value == 21.095
Test Passed: Value == 5.099
Test Passed: Value == 15.556
Test Passed: Value == 0
Test Passed: Value == 35856.153
*/
