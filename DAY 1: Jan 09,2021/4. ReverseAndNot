//We reverse 123 to get 321 and then add 123 to the end, resulting in 321123.
function reverseAndNot(i) {
var rev=0;
var copy_i=i;
while(copy_i!=0){
	rev=(rev*10)+(copy_i%10);
	copy_i=Math.floor(copy_i/10);
}
var res=rev.toString()+i.toString();
return parseInt(res);
}
