//question link: https://edabit.com/challenge/7ysTEDruHz2prcJQ9
function tuckIn(arr1, arr2) {
	var arr=[];
	
	 //arr=[...arr];
	arr.push(arr1[0]);
	arr.push(...arr2);
	arr.push(arr1[1]);
	return arr;
}
