Test Passed: Value == "['bottom', 'tomatosauce', 'vegetables', 'cheese', 'topping']"
Test Passed: Value == '[[1, 2], [3, 4], [5, 6]]'
Test Passed: Value == '[true, false, true, null, undefined, false]'
