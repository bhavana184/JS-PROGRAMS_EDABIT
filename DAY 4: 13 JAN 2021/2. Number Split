//https://edabit.com/challenge/xsi99TwpGyFC8KS6d
function numberSplit(n) {
	var arr=[];
	if(n%2==0)
	{
		arr.push(Math.floor(n/2));
	 arr.push(Math.floor(n/2));
	}
	else
	{
		arr.push(Math.floor(n/2));
	 	arr.push(Math.round(n/2));
	}
	return arr;
}
/*
Test Passed: Value == '[2, 2]'
Test Passed: Value == '[5, 5]'
Test Passed: Value == '[5, 6]'
Test Passed: Value == '[0, 0]'
Test Passed: Value == '[0, 1]'
Test Passed: Value == '[-2, -2]'
Test Passed: Value == '[-3, -2]'
Test Passed: Value == '[-5, -4]'
*/
