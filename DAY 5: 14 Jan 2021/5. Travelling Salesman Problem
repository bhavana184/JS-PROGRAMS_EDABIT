//https://edabit.com/challenge/r453y7c3vBQz4Rrbn
function fact(n){
	var f=1;
	for(var i=1;i<=n;i++)
		f=f*i;
	return f;
}
function paths(n) {
//	return n*paths(n-1);//factorial using recursion
	return fact(n);
}
/*
