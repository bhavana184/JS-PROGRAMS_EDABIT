//https://edabit.com/challenge/PXau3Fzk8GXgF6oRQ
function solveForExp(a, b) {
	return Math.floor(Math.log10(b)/Math.log10(a));
}
/*
Test Passed: Value == 5
Test Passed: Value == 10
Test Passed: Value == 10
Test Passed: Value == 16
Test Passed: Value == 9
Test Passed: Value == 6
Test Passed: Value == 8
*/
